   <!-- ActionSets -->
   <extension
         point="org.eclipse.ui.actionSets">      
      <actionSet
            id="org.eclipse.birt.report.designer.ui.publishActionSet"
            label="%ActionSet.Publish"
            visible="false">
         <action
               class="org.eclipse.birt.report.designer.ui.actions.PublishTemplateAction"
               id="org.eclipse.birt.report.designer.ui.TemplatepublisAction"
               label="%publish.template"
               menubarPath="file/export"
               style="push"/>
         <action
               class="org.eclipse.birt.report.designer.ui.actions.PublishLibraryAction"
               id="org.eclipse.birt.report.designer.ui.LibraryPublisAction"
               label="%publish.library"
               menubarPath="file/export"
               style="push"/>
      </actionSet>
   </extension>
    <extension
         point="org.eclipse.ui.menus">
      <menuContribution
          locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="undoredo.toolbar"
               label="Edit">
            <command
                  commandId="org.eclipse.ui.edit.undo"
                  id="undoredo.undo"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.edit.redo"
                  id="undoredo.redo"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   
