<report xmlns="http://www.eclipse.org/birt/2005/design" version="3.2.23" id="1">
    <property name="refreshRate">30</property>
    <styles>
        <style name="Test-Style" id="2"/>
    </styles>
    <page-setup>
        <graphic-master-page name="My Page" id="3">
            <property name="type">us-legal</property>
            <property name="orientation">landscape</property>
            <property name="topMargin">1in</property>
            <property name="leftMargin">1in</property>
            <property name="bottomMargin">2in</property>
            <property name="rightMargin">2in</property>
            <property name="columns">5</property>
            <property name="columnSpacing">2mm</property>
            <property name="backgroundColor">yellow</property>
            <property name="fontSize">12mm</property>
            <property name="color">red</property>
            <method name="onPageStart"><![CDATA[new script of onPageStart]]></method>
            <method name="onPageEnd"><![CDATA[new script of onPageEnd]]></method>            
            <contents>
                <label id="4"/>
            </contents>
        </graphic-master-page>
        <simple-master-page name="Simple MasterPage" id="5"/>
    </page-setup>
</report>
