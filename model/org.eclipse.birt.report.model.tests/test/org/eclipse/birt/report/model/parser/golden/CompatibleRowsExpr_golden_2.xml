            <property name="queryText">SELECT CLASSICMODELS.PRODUCTS.PRODUCTCODE,
       CLASSICMODELS.PRODUCTS.PRODUCTNAME,
       CLASSICMODELS.PRODUCTS.PRODUCTLINE,
       CLASSICMODELS.PRODUCTS.PRODUCTVENDOR,
