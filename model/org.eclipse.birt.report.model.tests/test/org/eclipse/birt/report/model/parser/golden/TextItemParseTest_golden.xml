<report xmlns="http://www.eclipse.org/birt/2005/design" version="3.2.23" id="1">
    <page-setup>
        <graphic-master-page name="My Page" id="2">
            <property name="columns">1</property>
            <property name="columnSpacing">0.25mm</property>
        </graphic-master-page>
    </page-setup>
    <body>
        <text name="text1" id="3">
            <expression name="altText" type="constant">Alt Text</expression>
            <property name="contentType">html</property>
            <text-property name="content" key="static"><![CDATA[new content hello <> <html></html>]]></text-property>
            <property name="hasExpression">false</property>
            <property name="tagType">Div</property>
            <property name="language">English</property>
            <property name="order">1</property>
        </text>
        <text name="text2" id="4">
            <text-property name="content"><![CDATA[    text &amp; < > ' " static    ]]&gt;]]></text-property>
        </text>
        <text name="text3" id="5">
            <text-property name="content" key="odd 1"><![CDATA[text &amp; &amp;lt; &amp;gt; &amp;apos; &amp;quot; static]]></text-property>
        </text>
        <text name="text4" id="6">
            <text-property name="content" key="static"><![CDATA[<hello>text &amp; </hello>&amp;lt; <hello>&amp;gt; &amp;apos; &amp;quot; static</hello>]]></text-property>
        </text>
    </body>
</report>
