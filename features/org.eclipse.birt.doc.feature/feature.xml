<feature
      id="org.eclipse.birt.doc"
      label="%featureName"
      version="4.17.0.qualifier"
      provider-name="%providerName"
      plugin="org.eclipse.birt.doc"
      license-feature="org.eclipse.license"
      license-feature-version="0.0.0">

   <description url="https://eclipse-birt.github.io/birt-website/">
      %description
   </description>

   <copyright url="%copyrightURL">
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="https://download.eclipse.org/birt/updates/release/latest"/>
      <discovery label="%updateSiteName" url="https://download.eclipse.org/birt/updates/release/latest"/>
   </url>

   <requires>
      <import plugin="org.eclipse.help" version="3.2.0" match="compatible"/>
      <import feature="org.eclipse.datatools.doc.user" version="1.0.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.eclipse.birt.doc"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
