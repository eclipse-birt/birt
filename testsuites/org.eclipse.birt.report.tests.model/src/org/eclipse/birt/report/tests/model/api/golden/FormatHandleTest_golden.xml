<?xml version="1.0" encoding="UTF-8"?>
<report xmlns="http://www.eclipse.org/birt/2005/design" version="3" id="1">
    <property name="author">W.C. Fields</property>
    <data-sources>
        <oda-data-source name="myDataSource"/>
    </data-sources>
    <styles>
        <style name="My Style">
            <property name="fontSize">12cm</property>
            <property name="dateTimeFormat">Short Date:yyyy/mm/dd</property>
            <property name="numberFormat">Standard:###,###.**</property>
            <property name="stringFormat">>:abcd</property>
            <list-property name="highlightRules">
                <structure>
                    <property name="dateTimeFormat">Medium Time:HH:MM:SS</property>
                    <property name="numberFormat">Percent:%##.##</property>
                    <property name="stringFormat">&lt;:ABC</property>
                </structure>
            </list-property>
        </style>
        <style name="Style1">
            <property name="dateTimeFormat">Short Date</property>
            <property name="stringFormat">></property>
            <list-property name="highlightRules">
                <structure>
                    <property name="dateTimeFormat">Short Time:customed</property>
                    <property name="numberFormat">Standard:###,###.##</property>
                    <property name="stringFormat">>:abcd</property>
                </structure>
            </list-property>
        </style>
    </styles>
    <page-setup>
        <graphic-master-page name="My Page">
            <contents>
                <label name="label1"/>
            </contents>
        </graphic-master-page>
    </page-setup>
</report>
