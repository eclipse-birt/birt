               driverPropertyID="org.eclipse.datatools.connectivity.db.password"
               driverTemplateID="org.eclipse.birt.report.data.oda.jdbc.dbprofile.sampledb.driverTemplate"
               id="org.eclipse.birt.report.data.oda.jdbc.dbprofile.sampledb.driverTemplate.password">
      </propertyEditor>
   </extension>

</plugin>
